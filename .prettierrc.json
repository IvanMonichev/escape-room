{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^types/(.*)$",
    "^hooks/(.*)$",
    "^store/(.*)$",
    "^services/(.*)$",
    "^assets/(.*)$",
    "^components/(.*)$",
    "^screens/(.*)$",
    "^utils/(.*)$",
    ".css$",
    "^[../]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
